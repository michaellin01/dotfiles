{
    "html.autoClosingTags": true,
    "html.format.indentInnerHtml": true,
    "python.jediEnabled": false,
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/15.0.6/clangd_15.0.6/bin/clangd",
    "semanticTokens.enable": true,
    "sql.database": "oracle",
    "snippets.userSnippetsDirectory": "~/.config/nvim/snippets",
    "diagnostic.checkCurrentLine": true,
    "diagnostic.virtualText": true,
    "diagnostic.virtualTextCurrentLineOnly": false,
    "diagnostic.virtualTextPrefix": "    \u2588 ",
    "java.home": "/usr/lib/jvm/java-13-openjdk-amd64",
    "java.trace.server": "verbose",
    "java.format.enabled": true,
    "java.import.maven.enabled": true,
    "java.maven.downloadSources": true,
    "suggest.noselect": true,
    "python.analysis.diagnosticSeverityOverrides": {
        "reportWildcardImportFromLibrary": "none"
    },
    "python.analysis.useLibraryCodeForTypes": true,
    "coc.preferences.rootPatterns": ["composer.json","composer.lock",".idea","package.json",".git",".hg",".projections.json"],
    "inlayHint.enable": false
